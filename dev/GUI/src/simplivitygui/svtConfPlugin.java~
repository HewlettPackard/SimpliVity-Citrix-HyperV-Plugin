    private void btnConfCancelActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnConfCancelActionPerformed
        // TODO add your handling code here:
        bsParentFrame.setVisible(true);
        this.dispose();
    }//GEN-LAST:event_btnConfCancelActionPerformed

    private void btnISOFileDlgActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnISOFileDlgActionPerformed
        // Code to open JFile Dialog and choose the ISO image 
        javax.swing.JFileChooser flInput = new javax.swing.JFileChooser(javax.swing.filechooser.FileSystemView.getFileSystemView().getHomeDirectory());
        flInput.showOpenDialog(this);
        java.io.File fName = flInput.getSelectedFile();
        txtISOLocation.setText(fName.toString());
        
    }//GEN-LAST:event_btnISOFileDlgActionPerformed

    private void btnPlugConfActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnPlugConfActionPerformed
        // TODO add your handling code here:
        if( isValidToProceed() == false ) {
           JOptionPane.showMessageDialog(this, "Required(*) fields cannot be empty", 
                                         "SimpliVity Plugin", JOptionPane.INFORMATION_MESSAGE);
        }
            
    }//GEN-LAST:event_btnPlugConfActionPerformed
    
